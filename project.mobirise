=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"row title-section\" mbr-if=\"showTitle\">\n                <div class=\"col-12\">\n                    <div class=\"header-container\">\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\" data-app-placeholder=\"Type Text\"><b>Last News</b></h2>\n                    </div>\n                </div>\n            </div>\n            <div class=\"row {{contentAlign}}\">\n                <div class=\"card col-12 col-lg-4\">\n                    <div class=\"card-body\">\n                        <img class=\"card-img mb-3\" src=\"@PROJECT_PATH@/assets/images/news3.jpg\" alt=\"Mobirise\" mbr-if=\"showImage\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Kindergarten Party</b></h2>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\">\n                            At molestie metus lectus eu tortor. Sed ultrices, ex ut sodales venena.</p>\n                    </div>\n                </div>\n                <div class=\"card col-12 col-lg-4\" mbr-if=\"colAmount > 1\">\n                    <div class=\"card-body\">\n                        <img class=\"card-img mb-3\" src=\"@PROJECT_PATH@/assets/images/news2.jpg\" alt=\"Mobirise\" mbr-if=\"showImage\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Outdoor Activity During Recess</b></h2>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\">\n                            Etiam nec arcu diam. Vestibulum porta, odio sit amet ultrices suscipit.</p>\n                    </div>\n                </div>\n                <div class=\"card col-12 col-lg-4\" mbr-if=\"colAmount > 2\">\n                    <div class=\"card-body\">\n                        <img class=\"card-img mb-3\" src=\"@PROJECT_PATH@/assets/images/news1.jpg\" alt=\"Mobirise\" mbr-if=\"showImage\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Birthday in Kindergarten</b></h2>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\">\n                            Praesent dolor nulla, sollicitudin nec odio congue, placerat rutrum sem.</p>\n                    </div>\n                </div>\n                <div class=\"card col-12 col-lg-4\" mbr-if=\"colAmount > 3\">\n                    <div class=\"card-body\">\n                        <img class=\"card-img mb-3\" src=\"@PROJECT_PATH@/assets/images/4-people.jpg\" alt=\"Mobirise\" mbr-if=\"showImage\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Learning Time With Katie</b></h2>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\">\n                            Mauris diam aliquet ipsum, vitae sollicitudin sapien orci dictum eros.</p>\n                    </div>\n                </div>\n                <div class=\"card col-12 col-lg-4\" mbr-if=\"colAmount > 4\">\n                    <div class=\"card-body\">\n                        <img class=\"card-img mb-3\" src=\"@PROJECT_PATH@/assets/images/5-people.jpg\" alt=\"Mobirise\" mbr-if=\"showImage\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Kindergarten Party</b></h2>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\">\n                            Proin maximus quam nulla, quis venenatis mi maximus vel.&nbsp;</p>\n                    </div>\n                </div>\n                <div class=\"card col-12 col-lg-4\" mbr-if=\"colAmount > 5\">\n                    <div class=\"card-body\">\n                        <img class=\"card-img mb-3\" src=\"@PROJECT_PATH@/assets/images/6-people.jpg\" alt=\"Mobirise\" mbr-if=\"showImage\">\n                        <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Outdoor Activity During Recess</b></h2>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .mbr-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\">\n                            Etiam quis auctor enim. Nam semper lacinia lectus a condimentum.&nbsp;</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
